{
  "name": "ng-game-publish",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run i18n && ng serve --port=4210 --proxy-config=proxy.config.json --host=0.0.0.0",
    "local": "ng serve --port=4210 --proxy-config=proxy.local.config.json --host=0.0.0.0",
    "build": "npm run i18n && ng build --prod --base-href=/isli/irms/web/ --rebase-root-relative-css-urls=true --named-chunks=true ",
    "dev": "npm run i18n && ng build  --base-href=/isli/irms/web/ --rebase-root-relative-css-urls=true ",
    "zh-i18n": "node i18n.js --dest=zh-cn.json --sname=\"(^zh.+\\.json$)|(.+zh\\.json$)\"",
    "en-i18n": "node i18n.js --dest=en.json --sname=\".*en\\.json$\"",
    "i18n": "npm run zh-i18n && npm run en-i18n",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-pro": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --prod --aot ---base-href=/isli/irms/web/ --rebase-root-relative-css-urls=true --named-chunks=true --statsJson=true",
    "analyz": "npm run build-pro && webpack-bundle-analyzer ./dist/mg-isli-ra-manager/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@nicky-lenaers/ngx-scroll-to": "^3.0.1",
    "classlist.js": "^1.1.20150312",
    "dom-scroll-into-view": "^1.2.1",
    "echarts": "^4.6.0",
    "file-saver": "^2.0.2",
    "html2canvas": "^1.0.0-rc.5",
    "mpr-form-valid": "^2.0.5",
    "ngx-clipboard": "^12.3.1",
    "ngx-echarts": "^4.2.2",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "xlsx": "^0.15.5",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "^8.1.3",
    "@types/echarts": "^4.4.3",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
